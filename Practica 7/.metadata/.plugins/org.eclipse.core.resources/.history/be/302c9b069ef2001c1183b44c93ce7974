package ar.edu.unlp.info.oo2.Ejercicio3;

public class Persona {

	private String nombre;
	
	public Persona(String nombre) {
		this.nombre = nombre;
	}

	public String getNombre() {
		return nombre;
	}
	
	public void El Proyecto se encuentra conformado no se puede cancelar.(Proyecto proyecto,double unValor) {
		proyecto.invertirDinero(unValor);
	}
	
}
